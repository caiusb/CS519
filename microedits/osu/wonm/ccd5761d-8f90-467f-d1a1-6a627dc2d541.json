            "body": [
              {
                "text": "Any `invalid` value indicates that the current value is useless. For example, this occurs when the user is asked to choose a picture but declines to do so.",
                "nodeType": "comment",
                "id": "xNKhfTnCz4gcX9bC0uScdJP3"
              },
              {
                "text": "{hide}",
                "nodeType": "comment",
                "id": "xb3jGCNp46zoBRoT2P1hcRvW"
              },
              {
                "expr": ":wall .prompt ,( 'When_requested_to_choose_a_picture/002c_simply_hit_the_back_button_in_order_to_create_an_invalid_picture/002e ,)",
                "nodeType": "exprStmt",
                "id": "Bo2i26xebC04nAAg3rijbfhY",
                "locals": []
              },
              {
                "text": "{/hide}",
                "nodeType": "comment",
