          "nodeType": "exprStmt",
          "locals": []
        },
        "zvxrLvGq9qAALGwxXQijifF4": {
          "expr": ":/267b #A6ec9DIrsh8D3XTcpY89AdVh #kXbEzip3We1Kr4u4OhBAi6SY ,( ,1 ,, :colors .black ,)",
          "nodeType": "exprStmt",
          "locals": []
