          "locals": []
        },
        "EidNXkkY4SAOcGDNjubgdiVG": {
          "text": "### scores",
          "nodeType": "comment"
        },
        "hpk4Dla4y2DBx4zGnT2PXPda": {
          "text": "Simply call `set score` to display a score on the top right corner of the board. You can use `score` to get back the current value of the score.",
          "nodeType": "comment"
        },
        "xLlJ2FcB2sQ7WQZz4RkNE3CX": {
          "expr": ":code #x7kqRhIkdf287CMs32vhQ5qE ,( ,3 ,6 ,0 ,0 ,)",
          "nodeType": "exprStmt",
          "locals": []
        },
        "vZsnWTwJxAwwUDsLVT6GD5ep": {
          "text": "You can also update the current score by using `add score`.",
          "nodeType": "comment"
        },
        "ojlfTT6CeEZPwdPnNElItnRv": {
          "expr": ":code #xsSEvGKnzsgYUBZUMTcNquOq ,( ,1 ,0 ,0 ,)",
