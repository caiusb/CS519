            "nodeType": "action",
            "body": [
              {
                "text": "Any invalid value indicates that the current value is useless. For example, this occurs when the user is asked to choose a picture but declines to do so.",
                "nodeType": "comment",
                "id": "WkoJm4YTOxBXxlOEx1xrOaHT"
              },
              {
                "expr": ":wall .prompt ,( 'When_requested_to_choose_a_picture/002c_simply_hit_the_back_button_in_order_to_create_an_invalid_picture/002e ,)",
                "nodeType": "exprStmt",
                "id": "g5llwREh2lXgUQxz3ZF5Cm3u",
                "locals": []
              },
              {
                "text": "snip:choosepicture",
                "nodeType": "comment",
                "id": "xxwF7sU4Nz93RYzhfuGnNJBD"
              },
              {
                "expr": "$IJudgjo4472G8vdRdW1Sbtx4L0 ,:= :media .choose_picture",
                "nodeType": "exprStmt",
                "id": "IJudgjo4472G8vdRdW1Sbtx4",
                "locals": [
                  {
                    "name": "pic",
                    "type": "Picture",
                    "nodeType": "localDef",
                    "id": "IJudgjo4472G8vdRdW1Sbtx4L0"
                  }
                ]
              },
              {
                "text": "endsnip",
                "nodeType": "comment",
                "id": "ef3aK9G3U4P2LMy6auIlMEyr"
              },
              {
                "text": "If you did not choose any picture, the returned picture will be invalid.",
                "nodeType": "comment",
                "id": "lAxdxdw2J6k9ooRKDSF84yW9"
              },
              {
                "text": "snip:isinvalid",
                "nodeType": "comment",
                "id": "oC6RD2CTxdQ4y7WrUYETAlP7"
              },
              {
                "nodeType": "if",
                "condition": "$IJudgjo4472G8vdRdW1Sbtx4L0 .is_invalid",
                "thenBody": [
                  {
                    "expr": "'This_picture_is_invalid/0021 .post_to_wall",
                    "nodeType": "exprStmt",
                    "id": "xA4cnfhqylFSvCpEPOFOs2wX",
                    "locals": []
                  }
                ],
                "isElseIf": false,
                "id": "x6ttegWrxnog8o5fTN0D71Dv",
                "locals": []
              },
              {
                "text": "endsnip",
                "nodeType": "comment",
                "id": "uZ5GOtJKrpvrKcOn06sUEMpd"
              },
              {
                "text": "Calling a property on an invalid value will fail.",
                "nodeType": "comment",
                "id": "qDFtDwo8x7Z1Xco84E94WOhg"
              },
              {
                "text": "snip:fail",
                "nodeType": "comment",
                "id": "xvLTx0Mh468ko2BOHluUx2gx"
              },
              {
                "expr": "$xUJChupzX1vL9MbjU243wAyrL0 ,:= $IJudgjo4472G8vdRdW1Sbtx4L0 .height",
                "nodeType": "exprStmt",
