            "body": [
              {
                "text": "Any value can be displayed to the user by calling the 'post to wall' action on it.",
                "nodeType": "comment",
                "id": "eWUgu7cna2gTlEYh0CiRIHzZ"
              },
              {
                "expr": "'Show_me/002e/002e/002e .post_to_wall",
                "nodeType": "exprStmt",
                "id": "DTt5sE69N2Kwyeok07RX3rup",
                "locals": []
              },
              {
                "text": "The wall displays the newest elements on top.",
                "nodeType": "comment",
                "id": "I7ZwtFc4oT6uQT7hYOrnWPmw"
              },
              {
                "expr": "'This_is_new_newer/002e/002e/002e .post_to_wall",
                "nodeType": "exprStmt",
                "id": "xSBflgnMTiHONMIdmfWR5jAb",
                "locals": []
              },
              {
                "text": "Some values have specialized display, e.g. a location is automatically displayed in a map.",
                "nodeType": "comment",
                "id": "yE0cBCkiM4cWOoeXb3LVTqsm"
              },
              {
                "expr": ":senses .current_location .post_to_wall",
                "nodeType": "exprStmt",
                "id": "P9WupmCRRyWzIHpWsU5jSTSj",
                "locals": []
              },
              {
                "text": "Or a picture album will show you thumbnails.",
                "nodeType": "comment",
                "id": "ZSwOxf8yLSwVAcaxaGShEdbg"
              },
              {
                "expr": ":media .saved_pictures .post_to_wall",
                "nodeType": "exprStmt",
                "id": "xPRgnvy8Qbs4NYD8qwdSip4p",
                "locals": []
              }
